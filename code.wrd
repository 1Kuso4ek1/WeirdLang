fun ifElseCheck() {
    if(10 < 5) -> println("Hello world")
    else if(10 > 5) -> println("Hi world!")
    else -> println("What?")
}

fun whileCheck(var start, var end) {
    var i = start

    while(i <= end) -> print(i++, " ")
    println()

    return i
}

fun main() {
    ifElseCheck()
    var i = whileCheck(5, 15)
    println("Returned: ", i)
}
