var arrSize = 10

fun fill(var arr)
{
    for(var i; i < arrSize; i++) # fill the array #
        a[i] = i
}

fun sum(var arr)
{
    for(var i; i < arrSize; i++)
        var _ += arr[i] # value of the last operation will be the returned one #
}

fun main()
{
    var a = alloc(arrSize) # allocate space for 10 elements #

    fill(a)

    println("sum: ", sum(a))

    free(a)
}
