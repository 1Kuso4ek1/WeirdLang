struct S
{
    fun S(var name)
    {
        this.name = name
        println("Constructed")
    }

    fun _S()
        println("Destroyed")

    var name
}

fun printNames(var s, var sPtr)
{
    println("s.name: ", s.name)
    println("sPtr.name: ", $sPtr.name)
    println("End of printNames")
}

fun main()
{
    var s = S("Hello")
    var sPtr = $s

    printNames(s, sPtr)
    println("End of main")
}
